{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/auth", "dest": "/api/auth.js" },
    { "src": "/api/test", "dest": "/api/test.js" },
    { "src": "/api/products", "dest": "/api/products.js" },
    { "src": "/api/email", "dest": "/api/email.js" },
    { "src": "/api/settings", "dest": "/api/settings.js" },
    { "src": "/", "dest": "/api/index.js" }
  ]
}
